{
  "name": "shopping-server",
  "version": "1.0.0",
  "description": "server",
  "main": "dist/index.js",
  "scripts": {
    "debug": "export DEBUG=* && tsc && node dist/index.js --unhandled-rejections=strict",
    "start": "node dist/index.js --unhandled-rejections=strict",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "migrate:init": "prisma migrate dev --name init",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "npm run docker:up && dotenv -e .env.test -- npm run migrate:init && dotenv -e .env.test -- jest -i --detectOpenHandles --watch",
    "migrate": "node-pg-migrate --envPath .env --tsconfig ./tsconfig.json"
  },
  "author": "Goran Tenic",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/connect-redis": "0.0.16",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.6",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/faker": "^5.5.6",
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^26.0.23",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.5",
    "@types/nodemailer": "^6.4.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "jest-mock-extended": "^1.0.16",
    "nodemon": "^2.0.9",
    "prettier": "^2.3.2",
    "prisma": "^2.27.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@prisma/client": "^2.27.0",
    "@types/pg": "^8.6.1",
    "argon2": "^0.28.2",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.12.0",
    "ioredis": "^4.27.6",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^6.0.0",
    "nodemailer": "^6.6.2",
    "pg": "^8.7.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "zod": "^3.10.3"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
