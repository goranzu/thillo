{
  "name": "shopping-server",
  "version": "1.0.0",
  "description": "server",
  "main": "dist/index.js",
  "scripts": {
    "debug": "export DEBUG=* && tsc && node dist/index.js --unhandled-rejections=strict",
    "start": "node dist/index.js --unhandled-rejections=strict",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "npm run docker:up && dotenv -e .env.test -- npm run migrate:init && dotenv -e .env.test -- jest -i --detectOpenHandles --watch",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback"
  },
  "author": "Goran Tenic",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/connect-redis": "0.0.18",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/faker": "^5.5.9",
    "@types/ioredis": "^4.28.7",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.8",
    "@types/nodemailer": "^6.4.4",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^6.6.6",
    "jest": "^27.4.7",
    "jest-mock-extended": "^2.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "argon2": "^0.28.3",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "ioredis": "^4.28.3",
    "knex": "^0.95.15",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^6.2.1",
    "nodemailer": "^6.7.2",
    "objection": "^3.0.1",
    "pg": "^8.7.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8"
  },
  "prettier": {
    "trailingComma": "all"
  }
}
